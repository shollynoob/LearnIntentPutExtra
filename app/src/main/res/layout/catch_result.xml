<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_main_result"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.visionstech.maidlib.CatchResult">

    <TextView
        android:text="TextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/catchphonenum"
        android:textSize="50sp"
        android:layout_marginTop="48dp"
        android:layout_below="@+id/catchaddress"
        android:layout_alignLeft="@+id/catchname"
        android:layout_alignStart="@+id/catchname" />

    <TextView
        android:text="TextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/catchaddress"
        android:textSize="50sp"
        android:layout_marginTop="56dp"
        android:layout_below="@+id/catchname"
        android:layout_alignLeft="@+id/catchphonenum"
        android:layout_alignStart="@+id/catchphonenum" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/catchphonenum"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="44dp"
        android:id="@+id/radiogroup"
        android:textSize="50sp"/>

    <TextView
        android:text="TextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/catchname"
        android:textSize="50sp"
        android:layout_marginTop="25dp"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true" />

</RelativeLayout>
